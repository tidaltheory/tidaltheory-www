    "rehype-stringify": "9.0.3",
    "remark-frontmatter": "4.0.1",
    "remark-parse": "10.0.1",
    "remark-rehype": "10.1.0",
    "stylelint": "15.10.2",
    "svelte": "4.1.2",
    "svelte-check": "3.4.6",
    "svelte-intersection-observer": "0.10.1",
    "svelte-preprocess": "5.0.4",
    "tailwindcss": "3.3.3",
    "tailwindcss-capsize": "3.0.3",
    "tailwindcss-opentype": "1.1.0",
    "unified": "10.1.2",
    "vite": "4.4.8"
