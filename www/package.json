  "devDependencies": {
    "@iarna/toml": "toml-1.0.0-rc.1",
    "@portabletext/svelte": "2.0.0",
    "@sveltejs/adapter-static": "2.0.3",
    "@sveltejs/kit": "1.22.4",
    "@tailwindcss/typography": "0.5.9",
    "@tidaltheory/lens": "1.2.0",
    "autoprefixer": "10.4.14",
    "globby": "13.1.3",
    "gray-matter": "4.0.3",
    "mdsvex": "0.11.0",
    "postcss": "8.4.27",
    "postcss-html": "1.5.0",
    "postcss-load-config": "4.0.1",
