    "lint:styles": "stylelint '**/*.{css,vue}' --ip '{.nuxt,dist}/**'",
    "package": "vite package",
    "preview": "vite preview"
  },
  "dependencies": {
    "@floating-ui/dom": "1.2.7",
    "date-fns": "2.29.3",
    "svelte-floating-ui": "1.2.8"
